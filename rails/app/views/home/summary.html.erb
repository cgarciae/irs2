<div class="row">
  <%= form_tag('/', class: "col s12", method: :get) do -%>

    <div class="section no-pad-bot" id="index-banner">
      <div class="container">
        <br><br>
        <h1 class="header center red-text text-lighten-1">Zoom In</h1>
        <div class="row center">
          <h5 class="header col s12 light">Select Images an press Summary when your done</h5>
        </div>
      </div>
    </div>

    <div class="container">
      <div class="">

        <div class="row">
          <div>
            <div class="col s12 m3">
 
              <div class="card">
                <div class="card-image waves-effect waves-block waves-light">
                  <img class="activator" src="<%= Backend::IMAGES_URL + session[:zoom].try(:[], :last_image) %>" height="200" >
                </div>
                <div class="card-content">
                  <p><a href="#"> </a></p>
                </div>
         
              </div>

            </div>
          </div>

        </div>
       
      </div>

    </div>

    <div class="container">
      <div class="section">
        <div class="row">
          <div class="col s12 m5"></div>
          <div class="col s12 m2">
            <button class="btn red lighten-1" type="submit" name="boton" value="zoomed" >Start over
              <i class="material-icons right">Continue</i>
            </button>

          </div>
          <div class="col s12 m5"></div>
        </div>
        <div class="row">
          <br/>
          <a href="/">volver</a>
        </div>
      </div>
    </div>

  <% end -%>
</div>
